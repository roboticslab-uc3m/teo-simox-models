<!--
	Robot Node Head
-->
	
	<RobotNodeSet name="HeadChain" kinematicRoot="AxialNeck" tcp="HeadTopTCP">
		<Node name="AxialNeck"/>
		<Node name="FrontalNeck"/>
	</RobotNodeSet>
	
</Robot>
